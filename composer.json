{
    "name": "ssntpl/neev",
    "description": "Enterprise-grade user authentication, team management, and multi-tenancy for Laravel SaaS applications.",
    "keywords": [
        "laravel",
        "authentication",
        "multi-tenancy",
        "teams",
        "mfa",
        "passkeys",
        "webauthn",
        "oauth",
        "sso",
        "rbac",
        "saas"
    ],
    "homepage": "https://github.com/ssntpl/neev",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/ssntpl/neev/issues",
        "source": "https://github.com/ssntpl/neev"
    },
    "autoload": {
        "psr-4": {
            "Ssntpl\\Neev\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ssntpl\\Neev\\NeevServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Abhishek Sharma",
            "email": "abhishek.sharma@ssntpl.in",
            "homepage": "https://ssntpl.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "geoip2/geoip2": "^3.3",
        "web-auth/webauthn-lib": "^5.2",
        "laravel/socialite": "^5.23",
        "spomky-labs/otphp": "^11.3",
        "ssntpl/laravel-acl": "^0.2",
        "bacon/bacon-qr-code": "^3.0"
    }
}
